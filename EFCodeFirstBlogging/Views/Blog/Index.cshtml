@model IEnumerable<EFCodeFirstBlogging.Models.Blog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


@using (Html.BeginForm())
{
    <table>
    @foreach (var item in Model)
    {
        <tr>
        <td>  @item.BlogId </td>
        <td>  @item.Name </td>
        <td>  @item.Url </td>
        </tr>
    }
    </table>   
}